{
  "name": "ty3status",
  "version": "0.3.2",
  "description": "A Typescript based i3 bar status program. Supports long running commands.",
  "main": "build/main.js",
  "author": "Kevin Gravier <kevin@mrkmg.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "lint": "tslint --force --format verbose \"src/**/*.ts\" \"test/**/*.ts\" ",
    "bin": "echo '#!/usr/bin/env node' > ./ty3status && cat ./build/ty3status.js >> ty3status && chmod +x ./ty3status",
    "build": "webpack",
    "build-watch": "webpack --watch",
    "install": "cp ./ty3status /usr/bin/ty3status && cp ./ty3status.yaml /etc/ty3status.yaml",
    "uninstall": "rm /usr/bin/ty3status && rm /etc/ty3status.yaml"
  },
  "dependencies": {
    "@types/js-yaml": "^3.5.28",
    "@types/minimist": "^1.1.29",
    "@types/node": "^6.0.46",
    "dateformat": "^1.0.12",
    "default-value": "^1.0.0",
    "humanize-duration": "^3.9.1",
    "js-yaml": "^3.6.1",
    "minimist": "^1.2.0",
    "on-build-webpack": "^0.1.0",
    "pad": "^1.0.1",
    "rimraf": "^2.5.4",
    "source-map-support": "^0.4.5",
    "ts-loader": "^0.9.4",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "webpack": "^1.13.2",
    "xtend": "^4.0.1"
  }
}
